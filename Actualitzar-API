void actualitzar_API(avio a[N],avio API[N],int i,int& cap){
     struct tm *info_temps=API[cap].data;
     int dia,mes,any,hora,minuts,segons;
     API[cap].codi==a[i].codi;
     API[cap].model==a[i].model;
     temps(info_temps,dia,mes,any,hora,minuts,segons);
     cap++;  //Dimensi√≥ de la taula API
}
