void actualitzar_API(avio a[N],APIavio A[N],int i,int& cap){
     struct tm *info_temps=A[cap].data;
     int dia,mes,any,hora,minuts,segons;
     A[cap].codi==a[i].codi;
     A[cap].model==a[i].model;
     temps(info_temps,dia,mes,any,hora,minuts,segons);
     cap++;  //Dimensi√≥ de la taula API
}
